          <cdtsEnvironment key="ESDCQA" path="http{0}://cdn-canada.services.gc.qat/{1}/cls/wet/{2}/{3}cdts/compiled/" env="esdcqat" />
     </cdtsEnvironments>
              
     <leavingSecureSiteWarning enabled="true" displayModalWindow="true" redirectURL="foo" excludedDomains=""></leavingSecureSiteWarning>
