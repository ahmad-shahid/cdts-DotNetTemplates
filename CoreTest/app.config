  <configSections>
    <section name="GoC.WebTemplate" type="GoC.WebTemplate.Configurations, WebTemplateCore" />
  </configSections>
  
  <!-- The following section is for testing purposes only -->
   <GoC.WebTemplate version="v4_0_23" 
                    theme="gcintranet" 
                    subTheme="esdc" 
                    environment="esdcprod" 
                    useHTTPS="true" 
                    loadJQueryFromGoogle="false" 
                    showPreContent="true" 
                    showPostContent="true" 
                    showFeedbackLink="true" 
                    feedbackLinkUrl="" 
                    showLanguageLink="true" 
                    showSharePageLink="true" 
                    showSearch="true"  
