@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public GoC.WebTemplate.Blazor.Model.RefFooter RefFooterObj { get; set; }

    protected override async void OnInitialized()
    {   
        await jsRuntime.InvokeVoidAsync("cdtsBlazor.setRefFooter", JsonPlainSerializationHelper.SerializeToJson(RefFooterObj).ToString(), RefFooterObj.ExitSecureSite);
    }
}