@inject IJSRuntime jsRuntime

@code {

    [Parameter]
    public Blazor.Model.RefFooter RefFooterObj { get; set; }

    protected override void OnInitialized()
    {   
        jsRuntime.InvokeVoidAsync("setRefFooter", JsonSerializationHelper.SerializeToJson(RefFooterObj).ToString(), RefFooterObj.ExitSecureSite);
    }
}