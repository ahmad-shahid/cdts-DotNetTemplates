@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public GoC.WebTemplate.Blazor.Model.RefFooter RefFooterObj { get; set; }

    protected override void OnInitialized()
    {   
        jsRuntime.InvokeVoidAsync("setRefFooter", JsonPlainSerializationHelper.SerializeToJson(RefFooterObj).ToString(), RefFooterObj.ExitSecureSite);
    }
}