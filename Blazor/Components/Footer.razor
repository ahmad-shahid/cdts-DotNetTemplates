@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public GoC.WebTemplate.Components.Entities.IFooter FooterObj { get; set; }

    protected override void OnAfterRender(bool firstRender)
    {
        if (FooterObj is GoC.WebTemplate.Blazor.Model.Footer) jsRuntime.InvokeVoidAsync("setFooter", JsonSerializationHelper.SerializeToJson(FooterObj).ToString(), false);
        else jsRuntime.InvokeVoidAsync("setFooter", JsonSerializationHelper.SerializeToJson(FooterObj).ToString(), true);
    }
}