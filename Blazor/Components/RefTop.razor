@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public GoC.WebTemplate.Blazor.Model.RefTop RefTopObj { get; set; }

    protected override void OnInitialized()
    {
        jsRuntime.InvokeVoidAsync("setRefTop", JsonPlainSerializationHelper.SerializeToJson(RefTopObj).ToString());
    }
}
