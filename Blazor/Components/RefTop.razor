@inject IJSRuntime jsRuntime

@code {
    [Parameter]
    public GoC.WebTemplate.Blazor.Model.RefTop RefTopObj { get; set; }

    protected override async void OnInitialized()
    {
        await jsRuntime.InvokeVoidAsync("cdtsBlazor.setRefTop", JsonPlainSerializationHelper.SerializeToJson(RefTopObj).ToString());
    }
}
