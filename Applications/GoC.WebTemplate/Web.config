<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <configSections>
    <section name="GoC.WebTemplate" type="GoC.WebTemplate.Configurations" />
  </configSections>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  
   <GoC.WebTemplate
      version="v4_0_23"
      theme="gcintranet"
      subTheme="esdc"
      environment="esdcprod"
      useHTTPS="true"
      loadJQueryFromGoogle="false"
          
      showPreContent="true"
      showPostContent="true"
      showFeedbackLink="true"
      feedbackLinkUrl=""
      showLanguageLink="true"
      showSharePageLink="true"
      showSearch="true"
      showFeatures="true"
      staticFilesLocation="~\GoC.WebTemplate\StaticFallbackFiles">
    
      <sessionTimeOut
        enabled="false"
        inactivity="1200000"
        reactionTime="180000"
        sessionalive="1200000"
        logouturl="./"
        refreshCallbackUrl="./"
        refreshOnClick="false"
        refreshLimit="120000"
        method=""
        additionalData=""></sessionTimeOut>
   
     <cdtsEnvironments>
          <cdtsEnvironment key="Akamai" path="http{0}://www.canada.ca/etc/designs/canada/cdts/{1}/{2}/cdts/compiled/" env="prod"/>

          <!--For Internal ESDC USE ONLY with GCIntranet Theme-->
          <cdtsEnvironment key="ESDCProd" path= "http{0}://templates.service.gc.ca/{1}/cls/wet/{2}/{3}cdts/compiled/" env="esdcprod"/>
          <cdtsEnvironment key="ESDCNonProd" path= "http{0}://s2tst-cdn-canada.sade-edap.prv/{1}/cls/wet/{2}/{3}cdts/compiled/" env="esdcnonprod"/>
          <cdtsEnvironment key="ESDCQA" path= "http{0}://cdn-canada.services.gc.qat/{1}/cls/wet/{2}/{3}cdts/compiled/" env="esdcqat"/>
     </cdtsEnvironments>
              
     <leavingSecureSiteWarning
       enabled="true"
       displayModalWindow="true"
       redirectURL="http://www.google.ca"
       excludedDomains=""></leavingSecureSiteWarning>
    </GoC.WebTemplate>

 
</configuration>
