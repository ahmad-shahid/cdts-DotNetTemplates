<configuration>
  <configSections>
    <section name="GoC.WebTemplate" type="GoC.WebTemplate.Configurations" />
  </configSections>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  
   <GoC.WebTemplate
                  xmlns="http://tempuri.org/WebTemplateWebForms.xsd"
      version="v4_0_25"
      environment="ESDC_PROD"
      loadJQueryFromGoogle="false"
      useHTTPS ="true"
      showPreContent="true"
      showPostContent="true"
      showFeedbackLink="true"
      feedbackLinkUrl=""
      showLanguageLink="true"
      showSharePageLink="true"
      showSearch="true"
      showFeatures="true"
      staticFilesLocation="~\GoC.WebTemplate\StaticFallbackFiles">
    
      <sessionTimeOut
        enabled="false"
        inactivity="1200000"
        reactionTime="180000"
        sessionalive="1200000"
        logouturl="./"
        refreshCallbackUrl="./"
        refreshOnClick="false"
        refreshLimit="120000"
        method=""
        additionalData=""></sessionTimeOut>
   
     <leavingSecureSiteWarning
       enabled="true"
       displayModalWindow="true"
       redirectURL="http://www.google.ca"
       excludedDomains=""></leavingSecureSiteWarning>
    </GoC.WebTemplate>

 
</configuration>
